{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "dom",
            "es7",
            "dom.iterable",
            "scripthost"
        ],
        "moduleResolution": "node",
        "module": "commonjs",
        "declaration": true,
        "removeComments": false,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    }
}